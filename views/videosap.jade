extends layout

block content
  #page-content.container
    .row
      .col-md-9.col-sm-12.content-holder
        // CONTENT
        #menu-container
          .container
              h1 Araling Panlipunan
              ul.nav.nav-tabs
                li
                  a(href='/videos') Search Video
                li
                  a(href='/videos/Science') Science
                li
                  a(href='/videos/Math') Mathematics
                li
                  a(href='/videos/Filipino/') Filipino
                li
                  a(href='/videos/English') English
                li.active
                  a(href='/videos/AralingPanlipunan') Araling Panlipunan
                li
                  a(href='/videos/Mapeh') Musics, Arts, P.E., Health
                li
                  a(href='/videos/Values') Values
              br

            .container.center#allLinks
            .col-md-15.col-sm-15
              .box-content2
                form.task-form(action='/videos', method='get')
                  .row
                    br
                    .col-md-10.col-md-offset-1
                      br
                      table.table-hover
                        br
                        tr
                          th(style='width:50%') Video Name
                          th(style='width:50%') Category
                          th(style='width:25%')

                        tbody#data
                            each video, i in videos
                              if (video.videoCategory == "AP")
                                tr
                                  td= video.videoTitle
                                  td= video.videoCategory
                                  td
                                    a.red-text(href= "/videos/" + video._id)
                                      button.btn.btn-warning(type='button') See Video
                                  p
                br
                p
                br
                p
                br       